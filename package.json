{
  "name": "@caldwell619/etl-helper",
  "version": "0.0.1",
  "description": "Helper framework for handling ETL syncs",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "source": "src/index.ts",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint ./ --ext .ts",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "build": "microbundle --compress --target node --tsconfig tsconfig.build.json",
    "bump:version": "standard-version",
    "publish:release": "git push --follow-tags origin master && yarn publish",
    "publish:version": "sh scripts/publish.sh",
    "predeploy": "cd demo && yarn install && yarn run build",
    "deploy": "gh-pages -d demo/build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:christopher-caldwell/etl-helper.git"
  },
  "bugs": {
    "url": "https://github.com/christopher-caldwell/etl-helper/issues"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "u/christopher-caldwell",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.3.1",
    "gh-pages": "^4.0.0",
    "jest": "29.0.3",
    "microbundle": "^0.13.3",
    "prettier": "^2.0.5",
    "rc": "^1.2.8",
    "standard-version": "^9.1.1",
    "ts-jest": "29.0.1",
    "ts-node": "9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "csv-parse": "^5.3.0",
    "xml2js": "^0.4.23"
  }
}
